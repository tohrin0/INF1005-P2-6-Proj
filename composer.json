{
  "name": "school-project/flight-booking-website",
  "description": "A flight booking website that utilizes a flight schedule API with responsive design and back-end functionality for user membership and data management.",
  "type": "project",
  "require": {
    "php": "^7.4 || ^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "vlucas/phpdotenv": "^5.0",
    "monolog/monolog": "^2.0",
    "phpmailer/phpmailer": "^6.9",
    "league/oauth2-google": "^4.0",
    "pragmarx/google2fa": "^8.0",
    "bacon/bacon-qr-code": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Classes\\": "classes/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8000"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
